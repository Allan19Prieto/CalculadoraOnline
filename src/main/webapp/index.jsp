<%@ page contentType="text/html; charset=UTF-8" pageEncoding="UTF-8" %>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>Calculadora_TEC</title>
    <link rel="stylesheet" href="styles.css">
</head>
<body>
<div>
<div style="margin: 100px 0 0 10px; color: #8696a5"  >
    <center>
        <div id="claculadora" class="text-center">

            <div>
                <span class="nombre"> Calculadora Online</span>
                <span class="nombre">----------------------------------------</span>

            </div>
            <form class="form-control">
                <div class="principal">
                    <div id="entrada" class="pantalla">
                        <span id="box">0</span>
                        <div class="c-rad">Rad</div>
                        <span class="clase1"></span>
                        <div class="mensaje"></div>
                        <div class="clase2"></div>
                        <div class="botones">
                            <div class="informacion">?</div>
                            <div id="mini" class="btn_mini">&gt;</div>
                            <div class="clase3">.</div>
                        </div>
                    </div>

                    <div class="calc-izquierda clase4">

                    </div>
                    <div class="calc-derecha ">

                        <div class="calc-black teclas ">
                            <div>7</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>8</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>9</div>
                        </div>

                        <div class="calc-brown teclas del-ac">
                            <div>AC</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>4</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>5</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>6</div>
                        </div>
                        <div class="calc-brown calc-f21 teclas teclas2">
                            <div>&times;</div>
                        </div>
                        <div class="calc-brown calc-f19 teclas teclas2">
                            <div>&divide;</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>1</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>2</div>
                        </div>
                        <div class="calc-black teclas">
                            <div>3</div>
                        </div>
                        <div class="calc-brown calc-f18 teclas teclas2 ">
                            <div>+</div>
                        </div>
                        <div class="calc-brown calc-f18 teclas teclas2">
                            <div>&#8211;</div>
                        </div>
                        <div class="calc-brown teclas  ">
                            <div>(</div>
                        </div>
                        <div class="calc-black calc-zero teclas">
                            <div>
                                <span>0</span>
                            </div>
                        </div>
                        <div class="calc-brown teclas  ">
                            <div>)</div>
                        </div>
                        <div class="calc-brown calc-f18 teclas teclas2">
                            <div>%</div>
                        </div>
                        <div class="calc-brown calc-f18 teclas teclas2">
                            <div><--</div>
                        </div>


                        <div class="calc-down equal teclas calc-orange calc-eq calc-f17 teclas2">=</div>

                        <textarea class="calc-blank"></textarea>

                    </div>
                </div>

            </form>
        </div>
    </center>
</div>
</div>
</body>
</html>